;AAIEO,EAAAA,KAAK,EAAG,eAAAC,KAAK;AAAA,WAAK;AAAEG,MAAAA,EAAE,EAAEH,KAAK,CAACE,MAAN,CAAaC;AAAnB,KAAL;AAAA;AAJf,CA3Ba,EAiCb;AACEP,EAAAA,IAAI,EAAE,MADR;AAEEC,EAAAA,IAAI,EAAE,OAFR;AAGEC,EAAAA,SAAS,EAAET;AAHb,CAjCa,CAAf;AAwCA,IAAMiB,MAAM,GAAG,IAAIpB,SAAJ,CAAc;AAC3BqB,EAAAA,IAAI,EAAE,SADqB;AAE3BC,EAAAA,IAAI,EAAEC,OAAO,CAACC,GAAR,CAAYC,QAFS;AAG3BhB,EAAAA,MAAM,EAANA;AAH2B,CAAd,CAAf,C,CAOA;;AACAW,MAAM,CAACM,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AACpC,MAAMV,YAAY,GAAGQ,EAAE,CAACG,OAAH,CAAWC,IAAX,CAAgB,UAAAC,CAAC;AAAA,WAAIA,CAAC,CAACd,IAAF,CAAOC,YAAX;AAAA,GAAjB,CAArB;;AAEA,MAAIA,YAAY,IAAI,CAACZ,IAAI,CAAC0B,WAA1B,EAAuC;AACrCJ,IAAAA,IAAI,CAAC,OAAD,CAAJ;AACD,GAFD,MAEO;AACLA,IAAAA,IAAI;AACL;AACF,CARD;AAUA,eAAeT,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport Home from '../views/Home.vue'\nimport About from '../views/About.vue'\nimport Error from '../views/Error.vue'\nimport Store from '../views/Store.vue'\nimport Auth from '../views/Auth.vue'\nimport WordView from '../views/WordView.vue'\n\nimport {auth} from '../firebaseConfig.js'\n\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/',\n    name: 'Home',\n    component: Home\n  },\n  {\n    path: '/about/:id',\n    name: 'About',\n    component: About, \n    props: function(route) {\n      return {userId: route.params.id}\n    }\n  },   \n  {\n    path: '/auth',\n    name: 'Auth',\n    component: Auth\n  },\n  {\n    path: '/store',\n    name: 'Store',\n    component: Store\n    ,\n    meta: { // connects with the function later in this file.\n      requiresAuth: true}\n  },\n  {\n    path: '/word/:id',\n    name: 'WordView',\n    component: WordView,\n    props:  route => ({ id: route.params.id })\n  },\n  {\n    path: '/404',\n    name: 'Error',\n    component: Error\n  }\n]\n\nconst router = new VueRouter({\n  mode: 'history',\n  base: process.env.BASE_URL,\n  routes\n})\n\n\n// this makes the router check each change in routes -- if their \"meta\" says it requires auth, it will check for auth.\nrouter.beforeEach((to, from, next) => {\n  const requiresAuth = to.matched.some(x => x.meta.requiresAuth)\n\n  if (requiresAuth && !auth.currentUser) {\n    next('/auth')\n  } else {\n    next()\n  }\n})\n\nexport default router\n"]}]}